@model FlowMonitoringInsp.Models.Site
@{
    ViewBag.Title = "Edit View";
}

<h2>Edit Site</h2>
@using (@Html.BeginForm())
{
    <div class="form-group">
        @Html.LabelFor(i => i.Address, htmlAttributes: new { @class = "control-label" })
        <div>
            @Html.EditorFor(i => i.Address, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(i => i.SiteTelogID, htmlAttributes: new { @class = "control-label" })
        <div>
            @Html.EditorFor(i => i.SiteTelogID, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
    <div class="form-group">
        @Html.HiddenFor(i => i.SiteID)
    </div>
    <div class="form-group">
        <div>
            <input type="submit" value="Save" class="btn btn-default" />
        </div>
    </div>
}
